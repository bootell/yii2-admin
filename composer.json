{
    "name": "bootell/yii2-admin",
    "description": "Admin system base on Yii 2",
    "keywords": ["yii2", "admin", "project template"],
    "homepage": "https://github.com/bootell/yii2-admin",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/bootell/yii2-admin"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii2": "~2.0.16",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "dmstr/yii2-adminlte-asset": "^2.6",
        "phpgangsta/googleauthenticator": "dev-master",
        "endroid/qr-code": "^3.7",
        "bower-asset/sweetalert": "^1.1",
        "kartik-v/yii2-widgets": "^3.4",
        "kartik-v/yii2-date-range": "^1.7"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "4.0.x-dev | ^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "phpunit/phpunit": "~5.7.27 || ~6.5.5",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
